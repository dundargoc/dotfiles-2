[
  {
    "location": "~/workspace/code/eclipse/eclipse.jdt.ls",
    "github": "eclipse/eclipse.jdt.ls",
    "cmds": [
      "./mvnw clean install -DskipTests",
      "rm -rf ~/.local/share/eclipse"
    ]
  },
  {
    "location": "~/apps/sjk.jar",
    "curl": "https://repository.sonatype.org/service/local/artifact/maven/redirect?r=central-proxy&g=org.gridkit.jvmtool&a=sjk&v=LATEST"
  },
  {
    "location": "~/apps/",
    "curl": "https://github.com/jvm-profiling-tools/async-profiler/releases/download/v1.4/async-profiler-1.4-linux-x64.tar.gz",
    "cmds": [
      "rm -rf ~/apps/async-profiler-1.4-linux-x64/",
      "aunpack ~/apps/async-profiler-1.4-linux-x64.tar.gz",
      "ln -sf ~/apps/async-profiler*/profiler.sh ~/bin/profiler.sh",
      "rm ~/apps/async-profiler-1.4-linux-x64.tar.gz"
    ]
  }
]
