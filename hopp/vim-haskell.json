[
    {
        "location": "~/.config/nvim/pack/plugins/start/",
        "github": [
            "eagletmt/ghcmod-vim"
        ],
        "cmds": [
            "stack install ghc-mod"
        ]
    },
    {
        "location": "~/.config/nvim/pack/plugins/start/",
        "github": "Shougo/vimproc.vim",
        "cmds": [
            "make"
        ]
    },
    {
        "location": "~/workspace/code/haskell/haskell-ide-engine",
        "github": "haskell/haskell-ide-engine",
        "cmds": [
            "stack install"
        ]
    },
    {
        "location": "/tmp/",
        "github": "ndmitchell/ghcid",
        "cmds": [
            "stack install ghcid",
            "cp -a /tmp/ghcid/plugins/nvim ~/.config/nvim/pack/plugins/start/ghcid"
        ]
    }
]
