[
    {
        "location": "~/workspace/code/haskell/haskell-ide-engine",
        "github": "haskell/haskell-ide-engine",
        "cmds": [
            "stack build --copy-compiler-tool"
        ]
    },
    {
        "location": "/tmp/",
        "github": "ndmitchell/ghcid",
        "cmds": [
            "stack install ghcid",
            "cp -a /tmp/ghcid/plugins/nvim ~/.config/nvim/pack/plugins/start/ghcid"
        ]
    }
]
