[
    {
        "location": "~/.local/share/nvim/mthesaur.txt",
        "curl": "https://raw.githubusercontent.com/words/moby/master/words.txt"
    },
    {
        "curl": "http://download.huzheng.org/dict.org/stardict-dictd_www.dict.org_gcide-2.4.2.tar.bz2",
        "location": "/tmp/",
        "cmds": [
            "mkdir -p ~/.local/share/stardict/",
            "tar xvf /tmp/stardict-dictd_www.dict.org_gcide-2.4.2.tar.bz2 -C ~/.local/share/stardict/ --strip 1",
            "rm -f /tmp/stardict-dictd_*.tar.bz2"
        ]
    },
    {
        "curl": "http://download.huzheng.org/dict.org/stardict-dictd-jargon-2.4.2.tar.bz2",
        "location": "/tmp/",
        "cmds": [
            "mkdir -p ~/.local/share/stardict/",
            "tar xvf /tmp/stardict-dictd-jargon-2.4.2.tar.bz2 -C ~/.local/share/stardict/ --strip 1",
            "rm -f /tmp/stardict-dictd_*.tar.bz2"
        ]
    }
]
