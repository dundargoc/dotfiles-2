[
    {
        "virtualenv": [
            "neovim",
            "flake8",
            "proselint",
            "neovim-remote",
            "jedi",
            "mistune",
            "psutil",
            "setproctitle"
        ],
        "location": "~/.virtualenvs/nvim",
        "cmds": [
            "ln -sf ~/.virtualenvs/nvim/bin/flake8 ~/bin/flake8",
            "ln -sf ~/.virtualenvs/nvim/bin/proselint ~/bin/proselint",
            "ln -sf ~/.virtualenvs/nvim/bin/nvr ~/bin/nvr"
        ]
    },
    {
        "location": "~/.config/nvim/autoload/",
        "curl": "https://tpo.pe/pathogen.vim"
    },
    {
        "location": "~/.config/nvim/bundle/fzf",
        "github": "junegunn/fzf",
        "cmds": "yes | sh install"
    },
    {
        "github": "fsharp/vim-fsharp",
        "location": "~/.config/nvim/bundle/vim-fsharp",
        "cmds": "make fsautocomplete"
    },
    {
        "location": "~/.config/nvim/bundle/",
        "github": [
            "roxma/nvim-completion-manager",
            "junegunn/fzf.vim",
            "tpope/vim-fugitive",
            "bling/vim-airline",
            "w0rp/ale",
            "mattn/webapi-vim",
            "mattn/gist-vim",
            "vim-scripts/linediff.vim",
            "hynek/vim-python-pep8-indent",
            "jamessan/vim-gnupg",
            "cespare/vim-toml",
            "tpope/vim-eunuch",
            "SirVer/ultisnips",
            "honza/vim-snippets",
            "vim-scripts/dbext.vim",
            "diepm/vim-rest-console",
            "ludovicchabant/vim-gutentags"
        ],
        "cmds": [
            "nvim +PluginInstall +UpdateRemotePlugins +qa"
        ]
    }
]
