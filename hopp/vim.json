[
    {
        "location": "vim/.vim/autoload/",
        "curl": "https://tpo.pe/pathogen.vim"
    },
    {
        "location": "vim/.vim/bundle/fzf",
        "github": "junegunn/fzf",
        "cmds": "yes | sh install"
    },
    {
        "location": "vim/.vim/bundle/ycm",
        "github": "Valloric/YouCompleteMe",
        "cmds": "python2 install.py --clang-completer --omnisharp-completer"
    },
    {
        "github": "fsharp/vim-fsharp",
        "location": "vim/.vim/bundle/vim-fsharp",
        "cmds": "make fsautocomplete"
    },
    {
        "location": "vim/.vim/bundle/",
        "github": [
            "junegunn/fzf.vim",
            "tpope/vim-sensible",
            "tpope/vim-fugitive",
            "bling/vim-airline",
            "scrooloose/syntastic",
            "scrooloose/nerdtree",
            "mattn/webapi-vim",
            "mattn/gist-vim",
            "vim-scripts/linediff.vim",
            "hynek/vim-python-pep8-indent",
            "jamessan/vim-gnupg",
            "cespare/vim-toml",
            "tpope/vim-eunuch",
            "SirVer/ultisnips",
            "honza/vim-snippets",
            "vim-scripts/dbext.vim",
            "diepm/vim-rest-console",
            "leafgarland/typescript-vim"
        ]
    }
]
