[
    {
        "virtualenv": [
            "neovim",
            "flake8",
            "proselint",
            "neovim-remote",
            "jedi",
            "psutil",
            "setproctitle"
        ],
        "location": "~/.virtualenvs/nvim",
        "cmds": [
            "ln -sf ~/.virtualenvs/nvim/bin/flake8 ~/bin/flake8",
            "ln -sf ~/.virtualenvs/nvim/bin/proselint ~/bin/proselint",
            "ln -sf ~/.virtualenvs/nvim/bin/nvr ~/bin/nvr"
        ]
    },
    {
        "location": "~/.config/nvim/pack/plugins/start/fzf",
        "github": "junegunn/fzf",
        "cmds": "yes | sh install"
    },
    {
        "location": "~/.config/nvim/pack/plugins/start/LanguageClient-neovim",
        "github": "autozimu/LanguageClient-neovim",
        "cmds": [
            "make release",
            "nvim +UpdateRemotePlugins +qa"
        ]
    },
    {
        "location": "~/.config/nvim/pack/plugins/start/",
        "github": [
            "roxma/nvim-completion-manager",
            "junegunn/fzf.vim",
            "tpope/vim-fugitive",
            "junegunn/gv.vim",
            "w0rp/ale",
            "mattn/webapi-vim",
            "mattn/gist-vim",
            "vim-scripts/linediff.vim",
            "hynek/vim-python-pep8-indent",
            "jamessan/vim-gnupg",
            "cespare/vim-toml",
            "tpope/vim-eunuch",
            "SirVer/ultisnips",
            "honza/vim-snippets",
            "vim-scripts/dbext.vim",
            "diepm/vim-rest-console",
            "ludovicchabant/vim-gutentags",
            "saltstack/salt-vim"
        ],
        "cmds": [
            "nvim +UpdateRemotePlugins +qa"
        ]
    }
]
